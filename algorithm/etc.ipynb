    "# 팩토리얼\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "print = sys.stdout.write\n",
    "\n",
    "# n = int(input())\n",
    "\n",
    "def factorial(num):\n",
    "    if num ==1 or num == 0:\n",
    "        return 1\n",
    "    return num * factorial(num-1)\n",
    "\n",
    "print(str(factorial(5)))"
